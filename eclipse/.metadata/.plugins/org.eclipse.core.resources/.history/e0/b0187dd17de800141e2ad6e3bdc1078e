package com.camp.item;

import net.minecraft.item.ItemRecord;
import net.minecraft.util.ResourceLocation;

import com.camp.creativetabs.CreativeTabsManager;
import com.camp.lib.StringLibrary;

import cpw.mods.fml.relauncher.Side;
import cpw.mods.fml.relauncher.SideOnly;

public class Record1 extends ItemRecord {

   private String setRecordName;
   protected Record1(String p_i45350_1_) {
      super(p_i45350_1_);
      // TODO Auto-generated constructor stub
   }
   {
this.setRecordName = "ikremix1";
this.maxStackSize = 1;
this.setCreativeTab(CreativeTabsManager.tabItem);
this.setUnlocalizedName("Record1");
   }
   @SideOnly(Side.CLIENT)
    public String getRecordTitle()
    {
        return this.recordName;
    }
    @Override
    public ResourceLocation getRecordResource(String name)
    {
    return new ResourceLocation(StringLibrary.MODID + ":records/ikremix1.ogg");
    }
}